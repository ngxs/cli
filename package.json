  "scripts": {
    "prepublishOnly": "tsc -p tsconfig.json && cp -R src/plop-helpers lib/src/plop-helpers",
    "// - Binaries": "Run against node_modules/.bin",
    "tsc": "tsc",
    "plop": "plop",
    "ts-node": "ts-node",
    "node-plop": "node-plop",
