    "test": "ts-mocha --paths -p ./tsconfig.json tests/*.spec.ts",
    "// - UTILS": "Utility Scripts",
    "lint": "tslint --exclude 'node_modules/**/*.ts' -c tslint.json '**/*.ts'"
